{
    "manifest_version": 3,
    "name": "ERGO Wallet Chrome Extension",
    "description": "A Chrome Extension Wallet for ERGO",
    "version": "1.0.0",
    "action": {
        "default_popup": "index.html",
        "default_title": "ERGO Wallet Chrome Extension"
    },
    "permissions": [
        "storage"
    ],
    "icons": {
        "16": "ergo_logo_16.png",
        "32": "ergo_logo_32.png",
        "48": "ergo_logo_48.png",
        "128": "ergo_logo_128.png",
        "192": "ergo_logo_192.png"
    }
}
