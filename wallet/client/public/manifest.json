{
    "manifest_version": 3,
    "name": "$PALM Wallet Chrome Extension",
    "description": "A Chrome Extension Wallet for $PALM",
    "version": "0.1.0",
    "action": {
        "default_popup": "index.html",
        "default_title": "$PALM Wallet Chrome Extension"
    },
    "permissions": [
        "storage"
    ],
    "icons": {
        "16": "palm_logo_16.png",
        "32": "palm_logo_32.png",
        "48": "palm_logo_48.png",
        "128": "palm_logo_128.png",
        "192": "palm_logo_192.png"
    }
}
